import java.util.Arrays;

class Reverse_Arr 
{
	public static void main(String[] args) 
	{
		int arr[]={10,20,30,40,50,60,70,80};
		System.out.println(Arrays.toString(arr));
		for(int i = 0,j=arr.length-1 ;i <= (arr.length)/2;i++,j--){
		int temp = arr[i];
		arr[i] = arr[j];
		arr[j] = temp;
			
		}
		System.out.println(Arrays.toString(arr));
	}
}
